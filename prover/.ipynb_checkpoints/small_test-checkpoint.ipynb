    "from z3 import *\n",
    "from z3_helper import Z3Helper\n",
    "\n",
    "Human = DeclareSort(\"Human\")\n",
    "is_mortal = Function(\"is_mortal\", Human, BoolSort())\n",
    "h = Const('h', Human)\n",
    "\n",
    "s = Solver()\n",
    "s.add([\n",
    "    ForAll([h], And(is_mortal(h), Not(is_mortal(h))))\n",
    "])\n",
    "\n",
    "print s.check()\n",
    "s.model()"
